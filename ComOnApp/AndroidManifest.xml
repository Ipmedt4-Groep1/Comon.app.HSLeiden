    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/comon_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="hsleiden.app.comon.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="hsleiden.app.comon.Main"
            android:label="@string/app_name" >
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="hsleiden.app.comon.Stemmen">
        </activity>
        <activity
            android:name="hsleiden.app.comon.Deelnemers">
        </activity>
        <activity
            android:name="hsleiden.app.comon.Kalender">
        </activity>
        <activity
            android:name="hsleiden.app.comon.Opdrachtgevers">
        </activity>
        <activity
            android:name="hsleiden.app.comon.Info">
        </activity>
        <activity
            android:name="hsleiden.app.comon.Nieuws">
        </activity>
    </application>

</manifest>
